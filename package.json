{
  "name": "admin-main",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix src --ext .js,.ts,.vue",
    "lint:lint-staged": "vue-tsc && lint-staged",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,scss,vue}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.0",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.1",
    "colord": "^2.9.3",
    "echarts": "^5.4.2",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.4",
    "qs": "^6.11.2",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@types/lodash-es": "^4.17.7",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@unocss/transformer-directives": "^0.53.4",
    "@vitejs/plugin-vue": "^4.1.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "5.0.3",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-mkcert": "^1.15.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(npm)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
